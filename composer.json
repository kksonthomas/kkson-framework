{
    "name": "kksonthomas/kkson-framework",
    "description": "self use framework",
    "license": "MIT",
    "authors": [
        {
            "name": "KKson Thomas",
            "email": "kksonthomas@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit --bootstrap phpunit_bootstrap.php --no-configuration src",
        "example-server": "@php -S localhost:3000",
        "test-php-cs-fixer": "php-cs-fixer fix example.php"
    },
    "minimum-stability": "stable",
    "require": {
        "ext-mbstring": "*",
        "php": ">=8.1.0",
        "gammadia/slim-2.x": "2.6.*",
        "league/plates": "^3.4.0",
        "gabordemooij/redbean": "^5.7.2",
        "components/jquery": "^1.11.0",
        "fortawesome/font-awesome": "4.*",
        "phpoffice/phpspreadsheet": "^1.23.0",
        "components/jqueryui": "^1.12.1",
        "greenlion/php-sql-parser": "4.*",
        "ircmaxell/password-compat" : "1.0.*",
        "voku/stringy": "^6.5.3",
        "bootstrap-select/bootstrap-select": "1.12.*",
        "moment/moment": "^2.29.3",
        "electrolinux/phpquery": "0.9.6",
        "phpmailer/phpmailer": "v5.2.14",
        "php-curl-class/php-curl-class": "4.10.0",
        "league/route": "2.*.*",
        "respect/validation": "1.1.14",
        "filp/whoops": "2.*.*",
        "swaggest/json-diff": "^3.5.0",
        "slim/slim": "2.6.*",
        "almasaeed2010/adminlte": "3.*.*",
        "ext-mysqli": "*",
        "gaomingcode/ckeditor5": "^27.1"
    },
    "autoload": {
        "psr-4": {
            "KKsonFramework\\" : "src/KKsonFramework"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "phpstan/phpstan": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.8",
        "gd-75/php-cs-fixer-enforce-double-quotes": "^1.0"
    }
}
